{
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "installCommand": "npm run vercel-install",
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api"
    }
  ],
  "functions": {
    "api/index.ts": {
      "includeFiles": "backend/**",
      "runtime": "nodejs18.x"
    }
  }
}
