{
  "version": 2,
  "buildCommand": "cd backend && npm install && npx prisma generate && npm run build",
  "installCommand": "cd backend && npm install",
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api"
    }
  ],
  "functions": {
    "api/index.ts": {
      "includeFiles": "backend/**",
      "runtime": "nodejs18.x"
    }
  }
}
